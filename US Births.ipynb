    "def read_csv(text):\n",
    "    wrap = open(text,'r')\n",
    "    data = wrap.read()\n",
    "    data = data.split('\\n')\n",
    "    head = data[0]\n",
    "    string_list = data[1:]\n",
    "    final_list = list()\n",
    "    for row in string_list:\n",
    "        string_fields = row.split(',')\n",
    "        int_fields = [int(x) for x in string_fields]\n",
    "        final_list.append(int_fields)\n",
    "    return final_list    "
   ]
